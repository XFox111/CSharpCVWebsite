@model Resume
@{
    Layout = null;
    ViewData["Title"] = "Print";
}

@Html.Raw(Model.Content)

<link rel="stylesheet" type="text/css" href="~/css/Style.css" />

<style>
    html {
        overflow: initial;
    }
    body {
        display: initial;
        margin: initial;
        height: initial;
    }
</style>

<script type="text/javascript">
    window.print();
    window.onfocus = function ()
    {
        window.location = "/Resume";
    }
</script>